      <link name="jaco_finger_link_2">
        <inertial> <!-- fnh -->
          <origin xyz="0 0 0" />
          <mass value="0.1" />
          <inertia  ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
