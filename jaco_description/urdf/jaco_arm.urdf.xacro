      </gazebo>
  
      <!-- Third finger (Approximative placing) -->

      <joint name="jaco_finger_joint_3" type="revolute">
        <origin xyz="-0.036 -0.021 ${-0.07}" rpy="0 ${-M_PI/7} 0"/>
        <axis xyz="0 1 0"/>
        <parent link="jaco_link_6"/>
        <child link="jaco_finger_link_3"/>
        <limit lower="-0.1" upper="${60*DEG2RAD}"  effort="10" velocity="1.0"/>
        <dynamics damping="50.5"/>
      </joint>
      <transmission name="jaco_finger_joint_3_transmission">
	<type>transmission_interface/SimpleTransmission</type>
        <actuator name="jaco_finger_joint_3_motor" >
	  <hardwareInterface>EffortJointInterface</hardwareInterface>
          <mechanicalReduction>50</mechanicalReduction>
